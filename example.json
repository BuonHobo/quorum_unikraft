{
    "benchmark": {
        "name": "quorum",
        "tx_rates": [
            100,
            150,
            200
        ],
        "duration": 40,
        "output_directory": "./data",
        "timeout": 30,
        "strategies": [
            {
                "name": "contract",
                "tx_size": 2
            },
            {
                "name": "baseline"
            }
        ],
        "processes": 3,
        "attempts": 5
    },
    "quorum": {
        "consensus": "raft",
        "contract": {
            "path": "contracts/IDS.sol",
            "version": "0.8.19",
            "params": 5,
            "numagents4params": 1,
            "extra_agents": [
                "0x2f6b58c01b28d5836cec6a38ee7cb00e758f500f",
                "0xc3b2b2d0dbef9b4c668656df9a97fc508e0bda2f"
            ],
            "events": {
                "01101": "sendAlert",
                "00000": "allQuiet"
            },
            "exec": [
                {
                    "method": "proposeNewValues",
                    "args": [
                        [
                            "P_0",
                            "P_2"
                        ],
                        [
                            1,
                            1
                        ]
                    ]
                }
            ]
        },
        "directory": "./.deployment",
        "toolboxContainer": "ubuntu-toolbox-24.10",
        "virtualizers": [
            {
                "name": "podman",
                "image": "docker.io/quorumengineering/quorum",
                "cpus": 1,
                "memory": "1G"
            },
            {
                "name": "host"
            },
            {
                "name": "unitest",
                "image": "buonhobo/geth",
                "memory": "1G"
            },
            {
                "name": "qemu",
                "qcow2": "build/bootc/qcow2/disk.qcow2",
                "user": "quorum",
                "key": "build/bootc/key",
                "memory": "1G",
                "cpus": 1
            }
        ],
        "nodes": [
            {
                "name": "node1",
                "role": "validator",
                "agent": true,
                "target": true,
                "verbosity": 0,
                "virtualizer": {
                    "name": "podman",
                    "cpus": 2
                }
            },
            {
                "name": "node2",
                "role": "validator",
                "verbosity": 0,
                "agent": true,
                "virtualizer": {
                    "name": "unitest"
                }
            },
            {
                "name": "node3",
                "role": "member",
                "verbosity": 2,
                "target": true,
                "virtualizer": {
                    "name": "qemu",
                    "memory": "512M"
                }
            }
        ]
    }
}