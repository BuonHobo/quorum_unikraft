{
    "benchmark": {
        "name": "quorum",
        "tx_rate": 200,
        "duration": 30,
        "output_file": "datatest/output.csv",
        "timeout": 30,
        "strategy": "contract",
        "processes": 3,
        "tx_size": 2
    },
    "quorum": {
        "consensus": "raft",
        "contract": {
            "path": "contracts/IDS.sol",
            "version": "0.8.19",
            "params": 5,
            "numagents4params": 1,
            "extra_agents": [
                "0xa8d8d247af547e38a7e1017116d93b6ce92d6212"
            ],
            "events": {
                "01001": "ciao",
                "01010": "hello"
            },
            "exec": [
                {
                    "method": "proposeNewValues",
                    "args": [
                        [
                            "P_2",
                            "P_1"
                        ],
                        [
                            3,
                            2
                        ]
                    ]
                }
            ]
        },
        "directory": "./deployment",
        "toolboxContainer": "ubuntu-toolbox-24.10",
        "virtualizers": [
            {
                "name": "podman",
                "image": "docker.io/quorumengineering/quorum",
                "cpus": 1,
                "memory": "1G"
            },
            {
                "name": "host"
            },
            {
                "name": "unitest",
                "image": "buonhobo/geth",
                "memory": "1G"
            },
            {
                "name": "qemu",
                "qcow2": "build/bootc/qcow2/disk.qcow2",
                "user": "quorum",
                "key": "build/bootc/key",
                "memory": "1G",
                "cpus": 1
            }
        ],
        "nodes": [
            {
                "name": "node1",
                "role": "validator",
                "agent": true,
                "target": true,
                "verbosity": 0,
                "virtualizer": {
                    "name": "unitest"
                }
            },
            {
                "name": "node2",
                "role": "validator",
                "verbosity": 0,
                "agent": true,
                "target": true,
                "virtualizer": {
                    "name": "unitest"
                }
            }
        ]
    }
}