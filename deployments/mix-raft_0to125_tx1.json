{
    "benchmark": {
        "name": "mix",
        "tx_rates": [
            75,100,125,150
        ],
        "duration": 40,
        "output_directory": "./data/mix",
        "timeout": 30,
        "strategies": [
            {
                "name": "contract",
                "tx_size": 1
            },
            {
                "name": "baseline"
            }
        ],
        "processes": 5,
        "attempts": 3,
        "worker": "worker"
    },
    "quorum": {
        "consensus": "raft",
        "contract": {
            "path": "contracts/IDS.sol",
            "version": "0.8.19",
            "params": 5,
            "numagents4params": 2
        },
        "directory": "./.deployment",
        "toolboxContainer": "ubuntu-toolbox-24.10",
        "virtualizers": [
            {
                "name": "podman",
                "image": "docker.io/quorumengineering/quorum",
                "cpus": 1,
                "memory": "1G"
            },
            {
                "name": "host"
            },
            {
                "name": "unitest",
                "image": "buonhobo/geth",
                "memory": "1G"
            },
            {
                "name": "qemu",
                "qcow2": "build/bootc/qcow2/disk.qcow2",
                "user": "quorum",
                "key": "build/bootc/key",
                "memory": "1G",
                "cpus": 1
            }
        ],
        "nodes": [
            {
                "name": "val1",
                "role": "validator",
                "verbosity": 3,
                "virtualizer": {
                    "name": "podman"
                }
            },
            {
                "name": "val2",
                "role": "validator",
                "verbosity": 3,
                "virtualizer": {
                    "name": "podman"
                }
            },
            {
                "name": "val3",
                "role": "validator",
                "verbosity": 3,
                "virtualizer": {
                    "name": "podman"
                }
            },
            {
                "name": "mem4",
                "role": "member",
                "agent": true,
                "target": true,
                "verbosity": 3,
                "virtualizer": {
                    "name": "unikraft"
                }
            },
            {
                "name": "mem5",
                "role": "member",
                "agent": true,
                "target": true,
                "verbosity": 3,
                "virtualizer": {
                    "name": "unikraft"
                }
            }
        ]
    }
}